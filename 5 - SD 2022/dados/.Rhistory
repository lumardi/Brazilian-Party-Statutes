.df %>%
rename(cat2 = .cat) %>%
group_by(cat2, !!!syms(.col)) %>%
filter(!is.na(cat2), !is.na(!!!syms(.col))) %>%
count() %>%
pivot_wider(names_from = cat2, values_from = n) %>%
mutate(across(everything(), ~ifelse(is.na(.x),0,.x))) %>%
mutate(name = .col) %>%
rename_with(.cols = 1, ~ " ") %>%
select(name, everything())
